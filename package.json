{
  "name": "naiveui-admin-template",
  "version": "0.0.1",
  "scripts": {
    "bootstrap": "yarn install",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .ts,vue src/** --no-error-on-unmatched-pattern",
    "commit": "git add -A && npx git-cz",
    "test-unit": "jest",
    "test-e2e": "cypress open",
    "test-e2e-ci": "cypress run",
    "test-ct": "cypress open-ct",
    "test-ct-ci": "cypress run-ct",
    "test": "yarn test-unit && yarn test-ct-ci && yarn test-e2e-ci"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --fix",
    "*": "prettier -w -u"
  },
  "dependencies": {
    "vue": "^3.0.5",
    "vue-router": "^4.0.8"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-jest": "^27.0.2",
    "chalk": "^4.1.1",
    "cypress": "^7.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.28.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.11.1",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "naive-ui": "^2.11.11",
    "prettier": "^2.3.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2",
    "vfonts": "^0.1.0",
    "vite": "^2.3.7",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.0.24"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}